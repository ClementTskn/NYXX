<robot name="test_robot">
    <link name="link1">
        <inertial>
            <origin xyz="1 1 1"></origin>
            <mass value="10"></mass>
            <inertia ixx="1" ixy="2" ixz="3" iyy="4" iyz="5" izz="6"></inertia>
        </inertial>
    </link>
    <link name="link2">
        <inertial>
            <origin xyz="1 0 0"></origin>
            <mass value="5"></mass>
            <inertia ixx="10" ixy="20" ixz="30" iyy="40" iyz="50" izz="60"></inertia>
        </inertial>
    </link>
    <link name="link3">
        <inertial>
            <origin xyz="0 1 0"></origin>
            <mass value="10"></mass>
            <inertia ixx="1" ixy="1" ixz="1" iyy="1" iyz="1" izz="1"></inertia>
        </inertial>
    </link>
    <link name="link4">
        <inertial>
            <origin xyz="1 1 0"></origin>
            <mass value="0.5"></mass>
            <inertia ixx="0.5" ixy="1" ixz="1.5" iyy="2" iyz="2.5" izz="3"></inertia>
        </inertial>
    </link>
 
<joint name="joint1" type="continuous">
    <origin xyz="0 1 0" rpy="0 0 3.14159265359"/>
    <parent link="link1"/>
    <child link="link2"/>

    <axis xyz="1 1 0"></axis>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
</joint>
 
    <joint name="joint2" type="continuous">
        <parent link="link1"/>
        <child link="link3"/>
    </joint>
    
    <joint name="joint3" type="continuous">
        <parent link="link3"/>
        <child link="link4"/>
    </joint>
</robot>