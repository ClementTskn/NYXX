QTableWidget
{
    outline: 0px;
}

QTableView::item,
QListView::item,
QTreeView::item
{
    padding: 3.0px;
}

QTableView::item:!selected:hover
{
    background-color: #353535;
    outline: 0;
    color: #efefef;
    padding: 4px;
}
QListView::item:!selected:hover,
QTreeView::item:!selected:hover
{
    background-color: #353535;
    outline: 0;
    color: #efefef;
    padding: 3.0px 3px 3px 2px;
    border-left: 3px solid #03c3f5;
}


QSlider::groove:horizontal
{
    border: 0px;
    height: 3.0px;
    background: #565a5e;
    margin: 0px;
}

QSlider::handle:horizontal
{
    background: #03c3f5;
    border: 0px;
    width: 10.0px;
    height: 10.0px;
    border-radius: 5px;
    margin: -4.0px 0;
}

QSlider::groove:vertical
{
    border: 0px;
    width: 3.0px;
    background: #565a5e;
    margin: 0px;
}

QSlider::handle:vertical
{
    border: 0px;
    background: #03c3f5;
    width: 10.0px;
    height: 10.0px;
    border-radius: 5px;
    margin: 0 -4.0px;
}

QSlider::handle:horizontal:hover,
QSlider::handle:horizontal:focus,
QSlider::handle:vertical:hover,
QSlider::handle:vertical:focus
{
    background: #03c3f5;
    border: 0px;
}

QSlider::sub-page:horizontal,
QSlider::add-page:vertical
{
    border: 0px;
    background: #03c3f5;
}

QSlider::add-page:horizontal,
QSlider::sub-page:vertical
{
    background: #626568;
}

QToolButton
{
    color: #efefef;
    background-color: #262626;
    border-width: 0px;
    padding: 5.0px;
    outline: none;
    margin: 3px;
}

QToolButton[popupMode="1"]  /* only for MenuButtonPopup */
{
    padding-right: 20px; /* make way for the popup button */
}

QToolButton[popupMode="2"]  /* only for InstantPopup */
{
    padding-right: 10px; /* make way for the popup button */
}

QToolButton::menu-indicator
{
    border-image: none;
    image: url(./GUI/dark/down_arrow.svg);
    top: -10.0px;
    left: -2.0px;
}

QToolButton::menu-arrow
{
    border-image: none;
    image: url(./GUI/dark/down_arrow.svg);
}

QToolButton::menu-button
{
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    /* 10px width + 4.0px for border + no text = 20px allocated above */
    width: 10px;
    padding: 5.0px;
    outline: none;
}

QPushButton::menu-indicator
{
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    left: 8.0px;
}

QTableView
{
    border: 1.0px solid #76797c;
    gridline-color: #1c1c1c;
    background-color: #161616;
}

QTableView,
QHeaderView
{
    border-radius: 0px;
}

QTableView::item:selected:active,
QTableView::item:pressed
{
    background: #353535;
    color: #efefef;
    padding : 0;
    border: 2px solid #03c3f5;
}

QListView::item:pressed,
QTreeView::item:pressed,
QTreeView::item:selected:active,
QListView::item:selected:active,
QListView::item:selected:hover,
QTreeView::item:selected:hover
{
    background-color: #03c3f5;
    color: #000000;
}

QHeaderView
{
    background-color: #1c1c1c;
    border: 1.0px transparent;
    border-radius: 0px;
    margin: 0px;
    padding: 0px;

}

QHeaderView::section
{
    background-color: #1c1c1c;
    color: #efefef;
    padding: 5.0px;
    border: 1.0px solid #2a2a2a;
    border-radius: 0px;
    text-align: center;
}

QHeaderView::section::vertical::first,
QHeaderView::section::vertical::only-one
{
    border-top: 1.0px solid #2a2a2a;
}

QHeaderView::section::vertical
{
    border-top: transparent;
}

QHeaderView::section::horizontal::first,
QHeaderView::section::horizontal::only-one
{
    border-left: 1.0px solid #2a2a2a;
}

QHeaderView::section::horizontal
{
    border-left: transparent;
}


QHeaderView::section:checked
{
    color: white;
    background-color: #334e5e;
}

 /* style the sort indicator */
QHeaderView::down-arrow
{
    image: url(./GUI/dark/down_arrow.svg);
}

QHeaderView::up-arrow
{
    image: url(./GUI/dark/up_arrow.svg);
}

QTableCornerButton::section
{
    background-color: #1c1c1c;
    border: 1.0px transparent #76797c;
    border-radius: 0px;
}

QToolBox
{
    padding: 5.0px;
    border: 1.0px transparent black;
}

QToolBox::item
{
    border: 0px;
}

QToolBox:selected
{
    background-color: #1c1c1c;
    border-color: #03c3f5;
}

QToolBox:hover
{
    border-color: #03c3f5;
}

QStatusBar::item
{
    border: 0px transparent dark;
}

QFrame[height="3"],
QFrame[width="3"]
{
    background-color: #76797c;
}

QSplitter::handle
{
    border: 1.0px dashed #2a2a2a;
}

QSplitter::handle:hover
{
    background-color: #787876;
    border: 1.0px solid #2a2a2a;
}

QSplitter::handle:horizontal
{
    width: 1.0px;
}

QSplitter::handle:vertical
{
    height: 1.0px;
}

QProgressBar:horizontal
{
    background-color: #626568;
    border: 1.0px solid #1c1c1c;
    height: 5.0px;
    text-align: right;
    margin-top: 5.0px;
    margin-bottom: 5.0px;
    margin-right: 50px;
    padding: 0px;
}

QProgressBar::chunk:horizontal
{
    background-color: #03c3f5;
    border: 1.0px transparent;
}

QSpinBox,
QDoubleSpinBox
{
    padding-right: 15.0px;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button
{
    subcontrol-origin: content;
    subcontrol-position: right top;

    width: 16.0px;
    border-width: 1.0px;
}

QSpinBox::up-arrow,
QDoubleSpinBox::up-arrow
{
    border-image: url(./GUI/dark/up_arrow.svg);
    width: 9.0px;
    height: 6.0px;
}

QSpinBox::up-arrow:hover,
QSpinBox::up-arrow:pressed,
QDoubleSpinBox::up-arrow:hover,
QDoubleSpinBox::up-arrow:pressed
{
    border-image: url(./GUI/dark/up_arrow-hover.svg);
    width: 9.0px;
    height: 6.0px;
}

QSpinBox::up-arrow:disabled,
QSpinBox::up-arrow:off,
QDoubleSpinBox::up-arrow:disabled,
QDoubleSpinBox::up-arrow:off
{
   border-image: url(./GUI/dark/up_arrow_disabled.svg);
}

QSpinBox::down-button,
QDoubleSpinBox::down-button
{
    subcontrol-origin: content;
    subcontrol-position: right bottom;

    width: 16.0px;
    border-width: 1.0px;
}

QSpinBox::down-arrow,
QDoubleSpinBox::down-arrow
{
    border-image: url(./GUI/dark/down_arrow.svg);
    width: 9.0px;
    height: 6.0px;
}

QSpinBox::down-arrow:hover,
QSpinBox::down-arrow:pressed,
QDoubleSpinBox::down-arrow:hover,
QDoubleSpinBox::down-arrow:pressed
{
    border-image: url(./GUI/dark/down_arrow-hover.svg);
    width: 9.0px;
    height: 6.0px;
}

QSpinBox::down-arrow:disabled,
QSpinBox::down-arrow:off,
QDoubleSpinBox::down-arrow:disabled,
QDoubleSpinBox::down-arrow:off
{
   border-image: url(./GUI/dark/down_arrow_disabled.svg);
}
